<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <filter>
        <filter-name>F1</filter-name>
        <filter-class>Filters.MyFilter</filter-class>
        <init-param>
            <param-name>my-param</param-name>
            <param-value>asdf</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>F1</filter-name>
        <url-pattern>/ExamPage</url-pattern>
        <dispatcher>REQUEST</dispatcher>
    </filter-mapping>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Servlet.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CourseBySession</servlet-name>
        <servlet-class>Servlet.CourseBySession</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExamPage</servlet-name>
        <servlet-class>Servlet.ExamPage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QuestionPage</servlet-name>
        <servlet-class>Servlet.QuestionPage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CourseBySession</servlet-name>
        <url-pattern>/CourseBySession</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExamPage</servlet-name>
        <url-pattern>/ExamPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QuestionPage</servlet-name>
        <url-pattern>/QuestionPage</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
